{
  "name": "sourcebroker/imageopt",
  "type": "typo3-cms-extension",
  "description": "Optimize images resized by TYPO3 so they take less space. TYPO3 services used. Cron based. Support for linux native commands and external services as Kraken.io etc.",
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms": "^6.2 || ^7.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "nimut/testing-framework": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "SourceBroker\\Imageopt\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SourceBroker\\Imageopt\\Tests\\": "Tests"
    }
  },
  "replace": {
    "imageopt": "self.version",
    "typo3-ter/imageopt": "self.version"
  }
}
